    $hwnd = $null
    for ($i = 0; $i -lt 10; $i++) {
        $process.Refresh()
        $hwnd = $process.MainWindowHandle
        if ($hwnd -ne 0) { break }
        Start-Sleep -Milliseconds 500
    }

    $process.WaitForExit()
