$runningPE = Get-Process -ErrorAction SilentlyContinue | Where-Object { $processNames -contains $_.ProcessName.ToLower() }

if ($runningPE) {
    Write-ColoredLine "  âœ“ Terminated existing Process Explorer instances." Green
    $runningPE | ForEach-Object {
        try {
            Stop-Process -Id $_.Id -Force -ErrorAction Stop
        } catch {}
    }
    Start-Sleep -Seconds 1
} else {
    Write-ColoredLine "  âœ“ No existing Process Explorer instances found." Green
}

$baseFolder = "C:\ToolsCLD"
