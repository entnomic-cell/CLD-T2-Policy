    Write-ColoredLine "CPU: $($cpu.Name)" Green
    Write-ColoredLine "‚úì Good performance." Green 
} else {
    Write-ColoredLine "CPU: $($cpu.Name)" Red
    Write-ColoredLine "‚ö† Slower scans possible." Yellow
}
